{
	"New PDF object": {
		"description": "New PDF object",
		"prefix": "obj",
		"body": [
			"$0X 0 obj",
			"<<",
			"   /Type ${1:/}",
			">>",
			"endobj",
			""
		]
	},
	"New PDF stream": {
		"description": "New PDF stream object",
		"prefix": "stream",
		"body": [
			"$0X 0 obj",
			"<<",
			"   /Length ${1:0}",
			">>",
			"stream",
			"",
			"endstream",
			"endobj",
			""
		]
	},
	"New xref table" : {
		"description": "New xref table",
		"prefix": "xref",
		"body": [
			"xref",
			"0 ${0:8}",
			"0000000000 65535 f",
			"0000000000 00000 n",
			"0000000000 00000 n",
			"0000000000 00000 n",
			"0000000000 00000 n",
			"0000000000 00000 n",
			"0000000000 00000 n",
			"0000000000 00000 n",
			"0000000000 00000 n",
			"trailer",
			"<<",
			"    /Size ${1:8}",
			"    /Root 1 0 R",
			">>",
			"startxref",
			"${2:0000}",
			"%%EOF",
			""
		]
	}
}