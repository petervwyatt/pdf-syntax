{
    "name": "pdf",
    "publisher": "petervwyatt",
    "displayName": "PDF",
    "description": "PDF Syntax Highlighter",
    "license": "MIT",
    "homepage": "https://github.com/petervwyatt/pdf-syntax",
    "icon": "icon.png",
    "version": "0.0.1",
    "author": {
		"name": "PDF Association"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/petervwyatt/pdf-syntax.git"
	},
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pdf",
                "aliases": [
                    "pdf", "pdf"
                ],
                "extensions": [
                    "pdf",
                    "fdf"
                ],
                "firstline": "^%PDF-[0-9]\\.[0-9]\b",
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pdf",
                "scopeName": "source.pdf",
                "path": "./syntaxes/pdf.tmLanguage.json"
            }
        ]
    }
}